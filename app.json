    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.joshreep.tempoconverter"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "Convert BPM to Milliseconds",
    "extra": {
      "eas": {
        "projectId": "b7b77200-3013-44d3-816b-f35d2bb04af9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
